| @font-face {
|   font-family: '#{font.fontname}';
|   src: url('#{font.fontname}.eot');
|   src: url('#{font.fontname}.eot?#iefix') format('embedded-opentype'),
|        url('#{font.fontname}.woff') format('woff'),
|        url('#{font.fontname}.ttf') format('truetype'),
|        url('#{font.fontname}.svg##{font.fontname}') format('svg');
|   font-weight: normal;
|   font-style: normal;
| }
| .font-#{font.fontname} {
|   font-family: '#{font.fontname}';
|   speak: none;
|   display: inline-block;
|   text-decoration: inherit;
|   width: 1em;
|   margin-right: .2em;
|   text-align: center;
| }
- var i, g, css, hex, chr
- for (i in glyphs)
  - g = glyphs[i]
  - css = meta.css_prefix + g.css
  - hex = '\\' + g.code.toString(16)
  - chr = unichr(g.code)
  != '\n'
  - if (0xffff < g.code)
    | span.font-#{font.fontname}.#{css}:before { content: '#{chr}'; } /* '#{hex}' */
  - else
    | span.font-#{font.fontname}.#{css}:before { content: '#{hex}'; } /* '#{chr}' */

